@using Autopark.DAL.Entities;
@model VehicleType;

@{
    ViewData["Title"] = "Create vehicle type";
}
<h1>@ViewData["Title"]</h1>
 <button class="btn btn-primary btn-outline-dark">
    <a style="color: white; text-decoration: none;" asp-controller="VehicleType" asp-action="Index">Back</a>
    </button>
<br />
<form asp-controller="VehicleType" asp-action="Create">
    <div class="form-group">
        <label class="col-1">Vehicle name</label>
        <input class="col-2" type="text" asp-for="TypeName" />
    </div>
    <div class="form-group">
        <label class="col-1">Coefficient of tax</label>
        <input class="col-2" asp-for="TaxCoefficient" />
    </div>
    <button class="btn btn-success btn-outline-dark" type="submit">Submit</button>
</form>

